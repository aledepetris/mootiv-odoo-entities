<record id="view_person_tree" model="ir.ui.view">
    <field name="name">mootiv.person.tree</field>
    <field name="model">mootiv.person</field>
    <field name="arch" type="xml">
        <tree>
            <field name="dni"/>
            <field name="name"/>
            <field name="last_name"/>
            <field name="email"/>
            <field name="telephone"/>
            <field name="birthdate"/>
            <field name="active"/>
        </tree>
    </field>
</record>
