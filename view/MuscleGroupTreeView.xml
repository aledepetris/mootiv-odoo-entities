<record id="view_muscle_group_tree" model="ir.ui.view">
    <field name="name">mootiv.muscle.group.tree</field>
    <field name="model">mootiv.muscle</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="is_muscle_group"/>
            <field name="muscles_ids" widget="many2many_tags"/>
        </tree>
    </field>
</record>
